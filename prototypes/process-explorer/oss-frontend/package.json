{
  "name": "@morgan-stanley/composeui-process-explorer-frontend-oss",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration production",
    "build:dev": "ng build --configuration development",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --watch=false",
    "test:dev": "ng test",
    "proxy": "npx @grpc-web/proxy --target http://localhost:5056 --listen 8056", 
    
    "proto2": "node ../../../node_modules/@grpc/proto-loader/build/bin/proto-loader-gen-types.js --longs=String --enums=String --defaults --oneofs --grpcLib=@grpc/grpc-js --outDir=proto/ ../dotnet/src/MorganStanley.ComposeUI.ProcessExplorer.Abstractions/Infrastructure/Protos/ProcessExplorerMessages.proto",
    "proto": "protoc --proto_path=../dotnet/src/MorganStanley.ComposeUI.ProcessExplorer.Abstractions/Infrastructure/Protos ProcessExplorerMessages.proto --js_out=import_style=commonjs,binary:./proto --grpc-web_out=import_style=typescript,mode=grpcweb:./proto"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.1.0",
    "@angular/cdk": "~16.1.7",
    "@angular/common": "^16.1.0",
    "@angular/compiler": "^16.1.0",
    "@angular/core": "^16.1.0",
    "@angular/forms": "^16.1.0",
    "@angular/material": "~16.1.7",
    "@angular/platform-browser": "^16.1.0",
    "@angular/platform-browser-dynamic": "^16.1.0",
    "@angular/router": "^16.1.0",
    "@grpc/proto-loader": "^0.7.10",
    "@types/google-protobuf": "3.15.10",
    "google-protobuf": "^3.21.2",
    "grpc-web": "^1.5.0",
    "protoc-gen-grpc-web": "^1.4.2",
    "protoc": "^1.1.3",
    "@grpc-web/proxy": "^0.1.0",
    
    "@grpc/grpc-js": "^1.9.9",
    "material-icons": "^1.13.10",
    "rxjs": "~7.8.0",
    
    "tslib": "^2.3.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.1.8",
    "@angular/cli": "~16.1.8",
    "@angular/compiler-cli": "^16.1.0",
    
    "@types/jasmine": "~4.3.0",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    
    "typescript": "~5.1.3"
  }
}
