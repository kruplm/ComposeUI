{
    "name": "@morgan-stanley/compose-messaging-client",
    "version": "0.1.0",
    "description": "JavaScript client for Compose UI's Message Router",
    "main": "index.js",
    "scripts": {
        "build": "yarn run build:clean && yarn run build:esm && yarn run build:cjs",
        "build:clean": "rimraf dist",
        "build:esm": "tsc --module ESNext --outDir dist/esm --project tsconfig.build.json",
        "build:cjs": "tsc --module CommonJS --outDir dist/cjs --project tsconfig.build.json",
        "test": "jest"
    },
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@jest/expect-utils": "^29.4.1",
        "@types/jest": "^29.4.0",
        "expect": "^29.4.1",
        "jest": "^29.4.2",
        "jest-extended": "^3.2.3",
        "rimraf": "^4.1.1",
        "ts-jest": "^29.0.5",
        "typescript": "^4.9.4"
    },
    "dependencies": {
        "@types/node": "^18.11.18",
        "rxjs": "^7.8.0"
    },
    "jest": {
        "preset": "ts-jest/presets/default-esm",
        "setupFilesAfterEnv": [
            "./testSetup.ts"
        ]
    },
    "dependenciesMeta": {
        "@jest/expect-utils@29.4.1": {
            "unplugged": true
        },
        "@types/jest@29.4.0": {
            "unplugged": true
        },
        "expect@29.4.1": {
            "unplugged": true
        },
        "jest@29.4.1": {
            "unplugged": true
        },
        "jest-extended@3.2.3": {
            "unplugged": true
        },
        "ts-jest@29.0.5": {
            "unplugged": true
        }
    }
}
